<html>
<body>
<div id="wrapper">
<h1>Twitter Clone 2.0</h1>
<h4>You have 7 followers</h4>
<%= link_to "New Tweet", new_tweet_path, :class => 'btn-new' %>
<table>
  <% @tweets.each do |tweet| %>
    <tr>
      <td><h1><%= tweet.title %></h1><br><%= tweet.description %><br><%= link_to "Edit", edit_tweet_path(tweet), :class => "btn-delete" %> <%= link_to "Delete", tweet, :method => :delete, :class => "btn-delete", data: { confirm: 'Are you sure you want to delete this tweet?' } %></td>
    </tr>
  <% end %>
</table>
<p>Copyright (C) 2016 Twitter Inc.</p>

</div>
</body>
</html>
